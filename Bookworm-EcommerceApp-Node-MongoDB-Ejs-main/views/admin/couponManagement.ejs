        <script>  
            function editCoupon(couponId, couponNameModal) {
                const editcouponName = document.getElementById("editcouponName").value;
                const editdiscountPercentage = document.getElementById("editdiscountPercentage").value;
                const editmaximumDiscountPrice = document.getElementById("editmaxDiscountPrice").value;
                const editminimumTotal = document.getElementById("editminTotalAmount").value;
                const ExpiredDate = new Date(document.getElementById('editexpDate').value);
                const editerrorElement = document.getElementById('editErrorMessage');


                function hideEditErrorMessage(){
                  editerrorElement.innerHTML="";
                }
                function showEditErrorMessage(message){
                  if (typeof editerrorElement !== "undefined" && editerrorElement !== null) {
                    editerrorElement.innerHTML = message;
                    editerrorElement.style.display = "block";
                  }
                  editerrorElement.innerHTML =`<div class="alert alert-warning border border-warning w-80 d-flex justify-content-center fw-bold py-2" role="alert" >${message}</div>`;
                    setTimeout(()=>{
                      editerrorElement.innerHTML =`<div></div>`
                    },5000);
                }

                if(editcouponName === "" || editcouponName.trim()==""){
                    showEditErrorMessage("Coupon Name field is empty");
                    return false;
                }

                if(editcouponName.length >30){
                    showEditErrorMessage("Coupon name must be less than 15");
                    return false;
                }



                if(editdiscountPercentage === "" || editdiscountPercentage.trim()==""){
                    showEditErrorMessage("Discount Percentage field is empty");
                    return false;
                }


                if(editdiscountPercentage > 100 || editdiscountPercentage <= 0){
                    showEditErrorMessage("Incorrect Discount Percentage");
                    return false;
                }


                if(editmaximumDiscountPrice === "" || editmaximumDiscountPrice.trim()==""){
                    showEditErrorMessage("Discount Percentage field is empty");
                    return false;
                }


                if(editmaximumDiscountPrice >= 100000 || editmaximumDiscountPrice <= 0){
                    showEditErrorMessage("Incorrect max DiscountPrice");
                    return false;
                }


                if(editminimumTotal === "" || editminimumTotal.trim()==""){
                    showEditErrorMessage("Discount Percentage field is empty");
                    return false;
                }


                if(editminimumTotal >= 100000 || editminimumTotal <= 0){
                    showEditErrorMessage("Incorrect Minimum Total Amount");
                    return false;
                }


                const edittoday = new Date();

                if (ExpiredDate < edittoday) {
                    showEditErrorMessage("Please Enter Valid Date");
                    return false;
                }

                hideEditErrorMessage()


                const url = window.location.origin;
                console.log(url);
                fetch(`${url}/admin/editCoupon`, {
                    method: 'POST',
                    headers: {
                        'Accept': 'application/json',
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({
                        couponId,
                        couponName : editcouponName,
                        discountPercentage : editdiscountPercentage,
                        maximumDiscountPrice : editmaximumDiscountPrice,
                        minimumTotal : editminimumTotal,
                        ExpiredDate
                    })
                }).then((response) => response.json())
                .then((data) => {
                    const modal = document.getElementById(couponNameModal);
                    modal.classList.remove('show');
                    modal.style.display = 'none';
                    const modalBackdrop = document.getElementsByClassName('modal-backdrop')[0];
                    modalBackdrop.parentNode.removeChild(modalBackdrop);

                    document.getElementById(`${couponId}`).innerHTML = 
                                      `<td><strong>${data.couponName}</strong></td>
                                      <td>${data.discountPercentage}</td>
                                      <td>${data.maximumDiscountPrice}</td>
                                      <td>${data.minimumTotal}</td>
                                      <td>${data.ExpiredDate}</td>
                                      <td><button type="button" class="dropdown-item" data-toggle="modal" data-target="#${data.couponName}" style="color:white; background-color: transparent;"><i class="bx bx-edit-alt me-1" style="color:white"></i> Edit</button></td>
                                      <td><a class="dropdown-item" onclick="couponDelete('${couponId}')" style="color:white; background-color: transparent;"><i class="bx bx-trash me-1" style="color:white"></i> Delete</a></td>`
                });
            }




            function couponDelete(couponId){
              const url = window.location.origin;
              console.log(url);
              fetch(`${url}/admin/deleteCoupon`, {
                method: 'DELETE',
                headers: {
                  'Accept': 'application/json',
                  'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    couponId
                })
              }).then((response)=>response.json())
              .then((data)=>{
                document.getElementById(`${couponId}`).remove()
              })
            }
        </script>
 

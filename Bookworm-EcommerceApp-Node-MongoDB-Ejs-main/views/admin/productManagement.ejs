                          <!-- Bootstrap Dark Table -->
                          <div class="card">
                            <h5 class="card-header">Product Management</h5>
                            <div class="table-responsive p-3">
                              <table class="table table-dark py-3" id="example">
                                <thead>
                                  <tr>
                                    <th>Book Name</th>
                                    <th>Book Details</th>
                                    <th>Other Information</th>
                                    <th>Images</th>
                                    <th>Status</th>
                                    <th>Actions</th>
                                  </tr>
                                </thead>
                                <tbody class="table-border-bottom-0">
                                  <% books.forEach((book) => { %>
                                  <tr>  
                                    <td><i class="fab fa-angular fa-lg text-danger me-3"></i> <strong><%= book.bookName %></strong></td>
                                    <td class="ps-5"><button type="button" class="btn btn-primary btn-xs" data-toggle="modal" data-target="#view<%= book._id %>">View</button></td>
                                      
                                      <!-- View Author Details -->
                                      <div class="modal fade" id="view<%= book._id %>" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                                        <div class="modal-dialog" role="document">
                                          <div class="modal-content">
                                            <div class="modal-header">
                                              <h5 class="modal-title" id="exampleModalLabel"><%= book.bookName %></h5>
                                              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                              </button>
                                            </div>
                                            <div class="modal-body">
                                              <p><%= book.bookDetails %></p>
                                             </div>
                                            <div class="modal-footer">
                                              <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                            </div>
                                          </div>
                                        </div>
                                      </div>

                                    <td class="ps-5"><button type="button" class="btn btn-light btn-xs" data-toggle="modal" data-target="#viewOtherInfo<%= book._id %>">View Other Info</button></td>

                                      <!-- View Other Info -->
                                      <div class="modal fade" id="viewOtherInfo<%= book._id %>" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                                        <div class="modal-dialog" role="document">
                                          <div class="modal-content">
                                            <div class="modal-header">
                                              <h5 class="modal-title" id="exampleModalLabel"><%= book.bookName %></h5>
                                              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                              </button>
                                            </div>
                                            <div class="modal-body">
                                              <span class="badge bg-danger p-2 mb-1">Author Name</span>
                                              <p><b><%= book?.author?.authorName %></b></p>
                                              <span class="badge bg-danger p-2 mb-1">Author Details</span>
                                              <p><%= book?.author?.authorDetails %></p>
                                              <div class="d-flex justify-content-between">
                                                <div>
                                                  <span class="badge bg-danger p-2 mb-1">Language</span>
                                                  <p><%= book.language %></p>
                                                </div>
                                                <div>
                                                  <span class="badge bg-danger p-2 mb-1">Retail Price</span>
                                                  <p><%= book.retailPrice %></p>
                                                </div>
                                                <div>
                                                  <span class="badge bg-danger p-2 mb-1">Rent Price</span>
                                                  <p><%= book.rentPrice %></p>
                                                </div>
                                              </div>
                                             </div>
                                            <div class="modal-footer">
                                              <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                            </div>
                                          </div>
                                        </div>
                                      </div>

                                    <td class="ps-4"><button type="button" class="btn btn-primary btn-xs" data-toggle="modal" data-target="#viewImage<%= book._id %>">View</button></td>

                                  <!-- View Image -->
                                  <div class="modal fade" id="viewImage<%= book._id %>" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                                    <div class="modal-dialog" role="document">
                                      <div class="modal-content">
                                        <div class="modal-header">
                                          <h5 class="modal-title" id="exampleModalLabel"><%= book.bookName %> - Images</h5>
                                          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                          </button>
                                        </div>
                                        <div class="modal-body">
                                          <div class="d-flex justify-content-around">
                                            <div class="d-flex justify-content-center border rounded border-dark p-3 my-2">
                                              <img src="../<%= book.image1 %>" alt="Image 1" class="me-2" style="width: 80px;">
                                              <button type="button" class="btn btn-xs" data-toggle="modal" data-target="#change1<%= book._id %>"><i class="bx bx-edit-alt me-1" ></i></button>
                                            </div>
                                            <div class="d-flex justify-content-center border rounded border-dark p-3 my-2">
                                              <img src="../<%= book.image2 %>" alt="Image 2" class="me-2" style="width: 80px;">
                                              <button type="button" class="btn btn-xs" data-toggle="modal" data-target="#change2<%= book._id %>"><i class="bx bx-edit-alt me-1" ></i></button>
                                            </div>
                                          
                                            <div class="d-flex justify-content-center border rounded border-dark p-3 my-2">
                                              <img src="../<%= book.image3 %>" alt="Image 3" class="me-2" style="width: 80px;">
                                              <button type="button" class="btn btn-xs" data-toggle="modal" data-target="#change3<%= book._id %>"><i class="bx bx-edit-alt me-1" ></i></button>
                                            </div>
                                          </div>
                                        </div>
                                        <div class="modal-footer">
                                          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                        </div>
                                      </div>
                                    </div>
                                  </div>
                                    <% if (book.delete) {%>
                                      <td><span class="badge bg-label-success me-1">Active</span></td>
                                    <% }else{ %>
                                      <td><span class="badge bg-label-warning me-1">In Active</span></td>
                                    <% } %>
                                    <td>
                                      <div class="dropdown">
                                        <button type="button" class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown">
                                          <i class="bx bx-dots-vertical-rounded"></i>
                                        </button>
                                        <div class="dropdown-menu">
                                          <button type="button" class="dropdown-item" data-toggle="modal" data-target="#edit<%= book._id %>"><i class="bx bx-edit-alt me-1"></i> Edit</button>
                                          <% if (book.delete) {%>
                                            <a class="dropdown-item" href="/admin/deleteBook/<%= book._id %>" ><i class="bx bx-block me-1"></i> Delete</a>
                                          <% }else{ %>
                                            <a class="dropdown-item" href="/admin/undeleteBook/<%= book._id %>"><i class="bx bx-lock-open-alt me-1"></i> Undelete</a>
                                          <% } %>
                                        </div>
                                      </div>
                                    </td>

                                        <!-- ============================================-->
                                        <!-- Edit Book Details ============================-->

                                        <div class="modal fade" id="edit<%= book._id %>" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                                          <div class="modal-dialog modal-dialog-centered" role="document">
                                            <div class="modal-content">
                                              <form id="editBook" action="/admin/editBook/<%= book._id %>" method="post" onsubmit="return editBookValidation()">
                                                <div class="modal-header">
                                                  <h5 class="modal-title" id="exampleModalLongTitle">Edit Book</h5>
                                                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                    <span aria-hidden="true">&times;</span>
                                                  </button>
                                                </div>
                                                <div class="modal-body">
                                                  <div id="editError"></div>
                                                  <div class="form-outline mb-1">
                                                    <label class="form-label" for="typeUsernameX-1">Enter Book</label>
                                                    <input type="text" id="bookName" class="form-control form-control-lg" name="bookName" value="<%=book.bookName %>"/>
                                                    <label class="form-label mt-3" for="typeUsernameX-1">Author Details</label>
                                                    <textarea name="bookDetails" class="form-control" id="bookDetails" style="height: 100px;"><%= book.bookDetails %></textarea>
                                                    <div class="form-group form-input pt-2">
                                                          <label for="author" style="color :#3e3e3e">Author:</label>
                                                          <select name="author" id="author" class="form-control form-control-lg dropdown" style="color :#3e3e3e">
                                                              <option value="<%= book?.author?._id %>" style="color :#3e3e3e"><%= book?.author?.authorName %></option>
                                                              <% authors.forEach((author) =>{ %>
                                                                  <option value="<%= author._id %>" style="color :#3e3e3e"><%= author.authorName %></option>
                                                              <% }) %>
                                                          </select>
                                                    </div>
                                                    <div class="form-group form-input pt-2">
                                                          <label for="genre" style="color :#3e3e3e">Genre:</label>
                                                          <select name="genre" id="genre" class="form-control form-control-lg dropdown" style="color :#3e3e3e">
                                                              <option value="<%= book?.genre?._id %>" style="color :#3e3e3e"><%= book?.genre?.genreName %></option>
                                                              <% genres.forEach((genre) =>{ %>
                                                                  <option value="<%= genre._id %>" style="color :#3e3e3e"><%= genre.genreName %></option>
                                                              <% }) %>
                                                          </select>
                                                    </div>
                                                    <div class="form-group form-input pt-2 w-100">
                                                      <label class="form-label" for="typeUsernameX-1" style="color :#3e3e3e">Language:</label>
                                                      <select name="language" id="language" class="form-control form-control-lg dropdown ps-2" style="color :#3e3e3e">
                                                          <option value="<%= book.language %>"><%= book.language %></option>
                                                          <option value="Acholi">Acholi</option>
                                                          <option value="Afrikaans">Afrikaans</option>
                                                          <option value="Akan">Akan</option>
